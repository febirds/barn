


.ux-tray-panel {
	background:transparent url(../images/default/taskbar/taskbar-bg.gif) repeat-x left top;
	left:0px;
	padding:0;
	position:absolute;
}
.ux-tray-panel .x-btn {
	float: left;
	margin: 1px 0 0 1px;
    position:relative;
}
/*vvvvv控制快速启动按钮和菜单的转换vvvvvv*/
.ux-quickstart-strip-wrap {
	width:100%;
    overflow:hidden;
    position:relative;
    zoom:1;
}

ul.ux-quickstart-strip {
	display:block;
	width:5000px;
    zoom:1;
}

ul.ux-quickstart-strip li {
    float:left;
    margin-left:0px;
}


ul.ux-quickstart-strip li.ux-quickstart-edge {
    float:left;
    margin:0 !important;
    padding:0 !important;
    border:0 none !important;
    font-size:1px !important;
    line-height:1px !important;
    overflow:hidden;
    zoom:1;
    background:transparent !important;
    width:1px;
}

.ux-taskbuttons-strip-wrap {
	width:100%;
    overflow:hidden;
    position:relative;
    zoom:1;
}

ul.ux-taskbuttons-strip {
	display:block;
	width:5000px;
    zoom:1;
}

ul.ux-taskbuttons-strip li {
    float:left;
    margin-left:2px;
}


ul.ux-taskbuttons-strip li.ux-taskbuttons-edge {
    float:left;
    margin:0 !important;
    padding:0 !important;
    border:0 none !important;
    font-size:1px !important;
    line-height:1px !important;
    overflow:hidden;
    zoom:1;
    background:transparent !important;
    width:1px;
}
/*^^^^^控制快速启动按钮和菜单的转换^^^^^*/
/* Begin QuickStart button */
.ux-tray-panel .x-btn-icon .x-btn-center .x-btn-text{
	background-position: center;
	background-repeat: no-repeat;
	height: 16px;
	width: 16px;
	cursor:pointer;
	white-space: nowrap;
    padding:0;
}
.ux-tray-panel .x-btn-left, .ux-tray-panel .x-btn-right{
	font-size:1px;
    line-height:1px;
}
.ux-tray-panel .x-btn-left{
	width:4px;
	height:28px;
	background:transparent /*url(../images/default/taskbar/quickstart-button.gif) no-repeat 0 0*/;
}
.ux-tray-panel .x-btn-right{
	width:4px;
	height:28px;
	background:transparent /*url(../images/default/taskbar/quickstart-button.gif) no-repeat 0 -28px*/;
}
.ux-tray-panel .x-btn-left i, .ux-tray-panel .x-btn-right i{
	display:block;
    width:4px;
    overflow:hidden;
    font-size:1px;
    line-height:1px;
}
.ux-tray-panel .x-btn-center{
	background:transparent /*url(../images/default/taskbar/quickstart-button.gif) repeat-x 0 -56px*/;
	vertical-align: middle;
	text-align:center;
	padding:0;
	cursor:pointer;
	white-space:nowrap;
}
.ux-tray-panel .x-btn-left{ 
	background-position:0 0;
}
.ux-tray-panel .x-btn-right{
	background-position:0 0;
}
.ux-tray-panel .x-btn-center{
	background-position:0 0;
}
.ux-tray-panel .x-btn-over .x-btn-left,
.ux-tray-panel .x-btn-menu-active .x-btn-left{
	background-position:0 -250px;
}
.ux-tray-panel .x-btn-over  .x-btn-right,
.ux-tray-panel .x-btn-menu-active .x-btn-right{
	background-position:0 -279px;
}
.ux-tray-panel .x-btn-over .x-btn-center,
.ux-tray-panel .x-btn-menu-active .x-btn-center{
	background-position:0 -308px;
}
.ux-tray-panel .x-btn-click .x-btn-left{
	background-position:0 -163px;
}
.ux-tray-panel .x-btn-click  .x-btn-right{
	background-position:0 -192px;
}
.ux-tray-panel .x-btn-click .x-btn-center{
	background-position:0 -221px;
}
.ux-tray-panel .active-win .x-btn-left{
	background-position:0 -84px;
}
.ux-tray-panel .active-win  .x-btn-right{
	background-position:0 -112px;
}
.ux-tray-panel .active-win .x-btn-center{
	background-position:0 -140px;
}
/* End: QuickStart button */
